{
  "name": "marketplace",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/marketplace",
  "projectType": "application",
  "tags": [],
  "targets": {
    "generate-data": {
      "dependsOn": ["skills-catalog:generate-registry"],
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx scripts/generate-data.ts",
        "cwd": "packages/marketplace"
      }
    },
    "build": {
      "dependsOn": ["generate-data"],
      "executor": "@nx/next:build",
      "outputs": ["{projectRoot}/.next"],
      "options": {
        "outputPath": "packages/marketplace/.next"
      }
    },
    "export": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/marketplace",
        "commands": ["next build"]
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "marketplace:build",
        "dev": true,
        "port": 3000,
        "turbopack": false
      },
      "configurations": {
        "development": {
          "buildTarget": "marketplace:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "marketplace:build:production",
          "dev": false
        }
      }
    }
  }
}
